{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@UserDTO": ["src/domain/user/user.dto"],
      "@UserService": ["src/domain/user/user.service"],
      "@IUserRepo": ["src/domain/user/interfaces/user.interface.repo"],
      "@IPostRepo": ["src/domain/post/interfaces/post.interface.repo"],
      "@IPostService": ["src/domain/post/interfaces/post.service.interface"],
      "@IUserService": ["src/domain/user/interfaces/user.service.interface"],
      "@UserDBRepo": ["src/infrastructure/database/user.db.repo"],
      "@UserDBRepoProvider": ["src/infrastructure/database/providers/userDBRepo.provider"],
      "@UserServiceProvider": ["src/domain/user/providers/userService.provider"],
      "@PostServiceProvider": ["src/domain/post/providers/postService.provider"],
      "@User": ["src/infrastructure/database/user.schema"],
      "@TypeORMModule": ["src/infrastructure/database/typeorm.module"],
    }
  }
}
